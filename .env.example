# MongoDB
MONGO_URI=mongodb://localhost:27017/airdrop

# Airdrop worker tuning
DOGE_AIRDROP_CONCURRENCY=5
DOGE_AIRDROP_MAX_RETRIES=3

# Dogecoin node RPC (required for CLI + airdrop processing)
# Set TESTNET to "true" to use Dogecoin testnet rules in CLI
TESTNET=true
# Default Dogecoin RPC URL: mainnet 22555, testnet 44555
NODE_RPC_URL=http://127.0.0.1:22555
NODE_RPC_USER=rpc_user
NODE_RPC_PASS=rpc_password

# Wallet file used by CLI (created with: npm run doge -- wallet new)
WALLET=.wallet.json

# Fee settings (CLI)
DYNAMIC_FEES=true
FEE_PER_KB=100000000

# Optional endpoints (CLI)
ORD=https://wonky-ord-v2.dogeord.io
DRC20_API=https://api.ordifind.com/drc20/balance
DOGGY_MARKET_API=https://api.doggy.market

